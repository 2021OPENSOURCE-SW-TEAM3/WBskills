      "img": "https://joeschmoe.io/api/v1/random",
      "url": "https://github.com/andreasbm/readme/blob/master/CONTRIBUTING.md"
    }
  ],
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "web-dialog": "0.0.8"
  },
  "devDependencies": {
    "snowpack": "^1.0.5"
  },
  "snowpack": {
    "dedupe": [
      "lit-html"
    ],
    "webDependencies": [
      "lit-html",
      "lit-element",
      "web-dialog",
      "lit-html/directives/repeat.js"
    ]
