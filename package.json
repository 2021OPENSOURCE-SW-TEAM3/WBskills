{
  "name": "web-skills",
  "description": "A comprehensive visual overview of skills you can learn as a web developer",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "serve": "http-server --push-state -o -c-1 -p 3011 -a localhost",
    "s": "npm run serve",
    "deploy": "firebase deploy",
    "lit-check": "npx lit-analyzer . --strict",
    "generate-blueprint": "node --experimental-modules generate-blueprint.mjs",
    "readme": "npm run generate-blueprint &&  npx @appnest/readme generate",
    "snowpack": "npx snowpack --optimize",
    "postinstall": "npm run snowpack"
  },
  "contributors": [
